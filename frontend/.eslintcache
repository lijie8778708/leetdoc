 {\n      this.setState({\n        msg: 'Missing username or password',\n        code: 400,\n        showPopup: !this.state.showPopup,\n      })\n    } else {\n      const config = {\n        headers: {\n          'Content-Type': 'application/json',\n        },\n      }\n      const body = JSON.stringify({\n        username: this.state.username,\n        password: this.state.password,\n      })\n\n      try {\n        axios.post('/user/login', body, config).then((res) => {\n          if (res.data.code === '200') {\n            this.setState({\n              msg: 'Welcome!',\n              showPopup: !this.state.showPopup,\n              code: 200,\n            })\n            //   this.props.handler(this.state)\n          } else if (res.data.code === '400') {\n            this.setState({\n              msg: res.data.data.msg,\n              showPopup: !this.state.showPopup,\n              code: 400,\n            })\n          }\n        })\n      } catch (err) {\n        console.log(err)\n      }\n    }\n  }\n  togglePopup() {\n    this.setState({\n      showPopup: !this.state.showPopup,\n    })\n    if (this.state.code === 200) {\n      this.props.handler('dashboard')\n    }\n  }\n  render() {\n    return (\n      <div>\n        <h4>Login In</h4>\n\n        <div className='form-group'>\n          <label>Username</label>\n          <input\n            type='text'\n            className='form-control'\n            placeholder='Enter username'\n            name='username'\n            value={this.state.username}\n            onChange={this.handleInputChange}\n          />\n        </div>\n\n        <div className='form-group'>\n          <label>Password</label>\n          <input\n            type='password'\n            className='form-control'\n            placeholder='Enter password'\n            name='password'\n            value={this.state.password}\n            onChange={this.handleInputChange}\n          />\n        </div>\n\n        <p className='forgot-password text-right'>\n          Not Registered Yet{' '}\n          <a style={{ color: 'blue' }} onClick={this.handleClick}>\n            sign up?\n          </a>\n        </p>\n        <button className='btn btn-primary btn-block' onClick={this.submit}>\n          Submit\n        </button>\n        {this.state.showPopup ? (\n          <Popup\n            text={this.state.msg}\n            closePopup={this.togglePopup.bind(this)}\n            button={this.state.code === 200 ? 'btn-primary' : 'btn-danger'}\n          />\n        ) : null}\n      </div>\n    )\n  }\n}\n\nexport default Login\n","/Users/jieli/Downloads/leetdoc/frontend/src/Auth/SignUp.js",["39","40"],"/Users/jieli/Downloads/leetdoc/frontend/src/Components/Popup.js",[],"/Users/jieli/Downloads/leetdoc/frontend/src/Components/Dashboard.js",[],{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","severity":1,"message":"46","line":4,"column":27,"nodeType":"47","messageId":"48","endLine":4,"endColumn":33},{"ruleId":"45","severity":1,"message":"49","line":4,"column":35,"nodeType":"47","messageId":"48","endLine":4,"endColumn":41},{"ruleId":"45","severity":1,"message":"50","line":4,"column":43,"nodeType":"47","messageId":"48","endLine":4,"endColumn":48},{"ruleId":"45","severity":1,"message":"51","line":4,"column":50,"nodeType":"47","messageId":"48","endLine":4,"endColumn":54},{"ruleId":"45","severity":1,"message":"52","line":10,"column":11,"nodeType":"53","messageId":"48","endLine":10,"endColumn":17},{"ruleId":"45","severity":1,"message":"54","line":22,"column":20,"nodeType":"47","messageId":"48","endLine":22,"endColumn":25},{"ruleId":"55","severity":1,"message":"56","line":32,"column":59,"nodeType":"57","messageId":"58","endLine":32,"endColumn":61},{"ruleId":"59","severity":1,"message":"60","line":110,"column":11,"nodeType":"61","endLine":110,"endColumn":67},{"ruleId":"55","severity":1,"message":"56","line":32,"column":59,"nodeType":"57","messageId":"58","endLine":32,"endColumn":61},{"r